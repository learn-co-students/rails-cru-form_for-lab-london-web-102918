<h1>Edit Song Form</h1>

<%= form_for(@song) do |f| %>
  <label>Edit Song Name:</label><br>
  <%= f.text_field :name %><br>

  <label>New Artist id:</label><br>
  <%= f.text_field :artist_id %><br>

  <label>New Genre id:</label><br>
  <%= f.text_field :genre_id %><br>

  <%= f.submit %>

<% end %>
